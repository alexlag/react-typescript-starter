{
  "name": "react-redux-typescript-webpack-starter",
  "version": "0.1.0",
  "description": "React / Redux / Typescript / Webpack2 - App Starter",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (https://piotrwitek.github.io/)",
  "homepage": "https://piotrwitek.github.io/react-redux-typescript-webpack-starter/",
  "repository": "https://github.com/piotrwitek/react-redux-typescript-webpack-starter.git",
  "bugs": "https://github.com/piotrwitek/react-redux-typescript-webpack-starter/issues",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack --config configs/webpack.config.js --env.build --env.sourceMap",
    "dev": "webpack-dev-server --config configs/webpack.config.js --open",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:update": "jest --config jest.json -u"
  },
  "dependencies": {
    "csstips": "^1.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "tslib": "^1.10.0",
    "typestyle": "^2.0.4"
  },
  "devDependencies": {
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "^4.39.1",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/webpack-env": "^1.14.0",
    "awesome-typescript-loader": "^5.2.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "react-hot-loader": "^4.12.14",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
